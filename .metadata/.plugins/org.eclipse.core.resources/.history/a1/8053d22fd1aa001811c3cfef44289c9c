package chapter1Exercises;

import javax.swing.JOptionPane;
public class NotReallyRandom {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		JOptionPane.showMessageDialog(null,"Guess a number between 1 and 100");
		JOptionPane.showMessageDialog(null,"The number is "+(1+ (int)(Math.random() * 100)));
		JOptionPane.showMessageDialog(null,"How far off were you?");
	}

}
